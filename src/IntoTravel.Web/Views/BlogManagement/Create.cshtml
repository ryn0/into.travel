@model IntoTravel.Web.Models.BlogManagementEntryModel
@{
    ViewData["Title"] = "Create New";
}

<h2>@ViewData["Title"]</h2>
 
<form asp-controller="BlogManagement" asp-action="Create" method="post" class="form-horizontal">
    
    @{ await Html.RenderPartialAsync("_blogEntry", Model); }


    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">Create</button>
        </div>
    </div>

</form>


@section ScriptsHeader {

    <script src="~/lib/ckeditor/ckeditor.js"></script>

    <script src="~/lib/ckeditor/config.js"></script>

}







@section ScriptsFooter {


    <script>
        CKEDITOR.replace('Content');
    </script>


}


