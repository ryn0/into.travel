@model IntoTravel.Web.Models.BlogManagementEntryModel
@{
    ViewData["Title"] = "Home";
}
 <p>Home @User.Identity.Name</p>


<form asp-controller="BlogManagement" asp-action="Create" method="post" class="form-horizontal">
     
    <div class="form-group">
        <label asp-for="Title" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="Title" class="form-control" />
            
        </div>
    </div>
 
   
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">Create</button>
        </div>
    </div>

</form>