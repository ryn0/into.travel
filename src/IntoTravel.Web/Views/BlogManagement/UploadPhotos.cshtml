@model IntoTravel.Web.Models.BlogPhotoUploadModel
@{
    ViewBag.Title = "Upload Photos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section>
    <div class="container">
 
        <h3>@ViewBag.Title</h3>



@using (Html.BeginForm( "UploadPhotosAsync", "BlogManagement", FormMethod.Post,
                new { enctype = "multipart/form-data" }))
{
   
    @Html.HiddenFor(x => x.BlogEntryId)

    <fieldset>
        <legend>Upload a file</legend>
        <div class="editor-field">
     
            <input type="file" class="multiple" name="files" multiple />
            <br /><br />
        </div>
        <div class="editor-field">
            <input class="btn btn-success btn-xs" type="submit" value="Upload" />
        </div>
    </fieldset>
}

    </div>
</section>